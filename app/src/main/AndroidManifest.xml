<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.co.rumahpanti.rumahpanti2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rumahpantilog"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PantiActivity" />
        <activity android:name=".InfakActivity" />
        <activity android:name=".KontakActivity" />
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name"
            android:noHistory="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Musikpad" />
        <activity android:name=".Bantuan" />
        <activity android:name=".UserActivity" />
        <activity
            android:name=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".ShowPanti" />
        <activity android:name=".ShowGallery"></activity>
    </application>

</manifest>